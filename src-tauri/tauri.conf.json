{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "template",
  "version": "0.1.0",
  "identifier": "com.template.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "template",
        "decorations": false,
        "width": 800,
        "height": 600,
        "visible": false
      },
      {
        "label": "float",
        "title": "悬浮搜索",
        "decorations": false,
        "width": 400,
        "height": 400,
        "url": "float.html",
        "transparent": true,
        "skipTaskbar": true,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}